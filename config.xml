<?xml version='1.0' encoding='utf-8'?>
<widget id="ru.oblafon.bescha" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>CordovaCodePush</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
       <access origin="https://codepush.azurewebsites.net" />
       <access origin="https://codepush.blob.core.windows.net" />
       <access origin="https://codepushupdates.azureedge.net" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="CodePushDeploymentKey" value="esEpDk9ExoXEviBEPnHHJImQNdepHJqhswT7z" />
        <preference name="CodePushPublicKey" value="esEpDk9ExoXEviBEPnHHJImQNdepHJqhswT7z" />
        <preference name="android-build-tool" value="gradle" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="CodePushDeploymentKey" value="Y-7IPNBd2f8034au26VxEIKWbS2mSyi6iD6Xf" />
        <preference name="CodePushPublicKey" value="Y-7IPNBd2f8034au26VxEIKWbS2mSyi6iD6Xf" />
    </platform>
    <engine name="android" spec="^6.3.0" />
    <engine name="ios" spec="^4.5.4" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-code-push" spec="^1.11.0" />
</widget>
